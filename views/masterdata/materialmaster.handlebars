<div>
  <h1>Material Master</h1>

  <h6>Find Kia Material : </h6><input id="autoComplete" />

  <br>
  <br>

  <div class="row">
    <div class="col s6">
      <form action="#">
        <div class="file-field input-field">
          <div class="btn">
            <span>File</span>
            <input type="file" id="fileinput1">
          </div>
          <div class="file-path-wrapper">
            <input class="file-path validate" type="text">
          </div>
          <div>
          </div>
        </div>
    </div>
    <div class="col s6">
      <a class="btn-floating btn-large waves-effect waves-light red disabled" id='uploadkia' onclick="uploadfile()"><i
          class="material-icons">add</i></a>
    </div>
  </div>


  </form>

  {{!-- <a class="waves-effect waves-light btn-middle" id='uploadkia' onclick="uploadfile()">UPLOAD</a> --}}


  <!-- button link is in onlink -->


  <table>
    <thead>
      <tr>
        <th>File Name</th>
        <th>Sync</th>
      </tr>
    </thead>

    <tbody>
      {{#each filelist as |file|}}
      <tr>
        <td>{{file}}</td>
        <td><button class='kia_apply_btn' data-filename={{file}}>Sync</button></td>
      </tr>
      {{/each}}
    </tbody>

  </table>



  <table>
    <thead>
      <tr>
        <th>Material</th>
        <th>Sap Material</th>
        <th>Description</th>
        <th>PAC</th>
      </tr>
    </thead>

    <tbody>
      {{#each kiamaterials as |kiamaterial|}}
      <tr>
        <td>{{kiamaterial.material}}</td>
        <td>{{kiamaterial.sapmaterial}}</td>
        <td>{{kiamaterial.description}}</td>
        <td>{{kiamaterial.pac}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>


  <table>
    <thead>
      <tr>
        <th>File Name</th>
        <th>Sync</th>
      </tr>
    </thead>

    <tbody>
      {{#each filelist as |file|}}
      <tr>
        <td>{{file}}</td>
        <td><button class='sap_apply_btn' data-filename={{file}}>Sync, Delete</button></td>
      </tr>
      {{/each}}
    </tbody>
  </table>

  <table>
    <thead>
      <tr>
        <th>Status</th>
        <th>Material</th>
        <th>SAPMaterial</th>
      </tr>
    </thead>

    <tbody>
      {{#each sapmaterials as |sapmaterial|}}
      <tr>
        <td>{{sapmaterial.status}}</td>
        <td>{{sapmaterial.material}}</td>
        <td>{{sapmaterial.sapmaterial}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>

</div>
<!--JavaScript at end of body for optimized loading-->
<script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
<script src="/js/js_in_handlebars/materialmaster/uploadfile_function.js"></script>
<script src="https://cdn.jsdelivr.net/npm/@tarekraafat/autocomplete.js@10.2.6/dist/autoComplete.min.js"></script>
<script src="/js/js_in_handlebars/materialmaster/materialmaster.js"></script>
<script src="/js/js_in_handlebars/materialmaster/autocomplete_component.js"></script>